   "source": [
    "### Q1. 주어진 데이터에서 10이상 20이하의 데이터만 출력하는 Series를 재정의 해보세요."
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 90,
   "outputs": [
    {
     "name": "stdout",
     "output_type": "stream",
     "text": [
      "HDD      19\n",
      "SSD      11\n",
      "USB       5\n",
      "CLOUD    97\n",
      "dtype: int64\n",
      "-------------------\n",
      "HDD    19\n",
      "SSD    11\n",
      "dtype: int64\n"
     ]
    }
   ],
   "source": [
    "import pandas as pd\n",
    "\n",
    "idx = [\"HDD\", \"SSD\", \"USB\", \"CLOUD\"]\n",
    "data = [19, 11, 5, 97]\n",
    "\n",
    "series = pd.Series(data, index=idx)\n",
    "\n",
    "print(series)\n",
    "print(\"-------------------\")\n",
    "series = series[(series >=10) & (series <= 20)]\n",
    "print(series)"
   ],
