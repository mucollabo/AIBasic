    "num_list = [1, 5, 7, 15, 16, 22, 28, 29]\n",
    "\n",
    "def get_odd_num(num_list):\n",
    "    for num in num_list:\n",
    "        if num % 2 == 0:\n",
    "            num_list.remove(num)\n",
    "    return num_list\n",
    "\n",
    "print(get_odd_num(num_list))"
